<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SimpleViewBehavior">
        <attr name="svb_dependOn" format="reference" />
        <attr name="svb_dependType">
            <enum name="height" value="0" />
            <enum name="width" value="1" />
            <enum name="x" value="2" />
            <enum name="y" value="3" />
        </attr>
        <attr name="svb_dependTargetX" format="reference|dimension" />
        <attr name="svb_dependTargetY" format="reference|dimension" />
        <attr name="svb_dependTargetWidth" format="reference|dimension" />
        <attr name="svb_dependTargetHeight" format="reference|dimension" />
        <attr name="svb_targetX" format="reference|dimension" />
        <attr name="svb_targetY" format="reference|dimension" />
        <attr name="svb_targetWidth" format="reference|dimension" />
        <attr name="svb_targetHeight" format="reference|dimension" />
        <attr name="svb_targetBackgroundColor" format="reference|color" />
        <attr name="svb_targetAlpha" format="float" />
        <attr name="svb_targetRotateX" format="float" />
        <attr name="svb_targetRotateY" format="float" />
        <attr name="svb_animation" format="reference" />

        <!--定义名称-->
        <attr name="textFieldStyle" format="reference" />
    </declare-styleable>

    <declare-styleable name="ColumnView">
        <attr name="column_color" format="color" />
    </declare-styleable>

    <declare-styleable name="SuperEditText">
        <attr name="ic_delete" format="reference" />
        <attr name="delete_x" format="integer"  />
        <attr name="delete_y" format="integer"  />
        <attr name="delete_width" format="integer"  />
        <attr name="delete_height" format="integer"  />

        <attr name="ic_left_click" format="reference" />
        <attr name="ic_left_unclick" format="reference" />
        <attr name="left_x" format="integer"  />
        <attr name="left_y" format="integer"  />
        <attr name="left_width" format="integer"  />
        <attr name="left_height" format="integer"  />

        <attr name="lineColor_click" format="color"  />
        <attr name="lineColor_unclick" format="color"  />
        <attr name="linePosition" format="integer"  />

        <attr name="cursor" format="reference" />
    </declare-styleable>

    <declare-styleable name="CustomPasswordView">
        <attr name="npvTextColor" format="color|reference" />
        <attr name="npvTextSize" format="dimension" />
        <attr name="npvLineColor" format="color" />
        <attr name="npvGridColor" format="color" />
        <attr name="npvLineWidth" format="dimension" />
        <attr name="npvPasswordLength" format="integer" />
        <attr name="npvPasswordTransformation" format="string" />
        <attr name="npvPasswordType" format="enum">
            <enum name="numberPassword" value="0" />
            <enum name="textPassword" value="1" />
            <enum name="textVisiblePassword" value="2" />
            <enum name="textWebPassword" value="3" />
        </attr>
    </declare-styleable>
</resources>
